package vasanth;
import java.util.*;
public class kaprekar {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n,sq;
		String s,s1,s2;
		n=sc.nextInt();
		sq=n*n;
		s=String.valueOf(sq);
		s1=s.substring(0,2);
		s2=s.substring(2,4);
		int x=Integer.parseInt(s1);
		int y=Integer.parseInt(s2);
		int val=x+y;
		if (val==n)
		{
			System.out.println("YES");
		}
		else
		{
			System.out.println("NO");
		}
	}

}
